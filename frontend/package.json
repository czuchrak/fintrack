{
  "name": "fintrack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "rimraf ./build && env-cmd -f .env.development react-scripts start",
    "demo": "rimraf ./build && env-cmd -f .env.demo react-scripts start",
    "build:test": "env-cmd -f .env.test react-scripts build && shx rm -rf ClientApp && shx cp -r build ClientApp",
    "build:demo": "env-cmd -f .env.demo react-scripts build && shx rm -rf ClientApp && shx cp -r build ClientApp",
    "build:production": "env-cmd -f .env.production react-scripts build && shx rm -rf ClientApp && shx cp -r build ClientApp",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx ./src",
    "lint:fix": "eslint --fix --ext .js,.jsx ./src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
